{
  "threshold": 0,
  "reporters": ["json", "console"],
  "ignore": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/node_modules/**",
    "**/dist/**",
    "**/coverage/**",
    "**/.stryker-tmp/**",
    "**/example/**"
  ],
  "format": ["typescript", "javascript"],
  "absolute": true,
  "gitignore": true,
  "minLines": 5,
  "minTokens": 50,
  "output": "./reports/duplication"
}
