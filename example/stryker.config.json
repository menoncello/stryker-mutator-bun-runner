{
  "testRunner": "command",
  "commandRunner": {
    "command": "bun test"
  },
  "mutate": [
    "src/**/*.ts",
    "!src/**/*.test.ts"
  ],
  "logLevel": "info",
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": 50
  },
  "coverageAnalysis": "perTest",
  "tempDirName": ".stryker-tmp",
  "cleanTempDir": true
}