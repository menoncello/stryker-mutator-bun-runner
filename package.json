{
  "name": "@stryker-mutator/bun-runner",
  "version": "0.3.0",
  "description": "A test runner plugin to use Bun with StrykerJS, the JavaScript mutation testing framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "test": "bun test test/",
    "test:watch": "bun test test/ --watch",
    "test:coverage": "bun test test/ --coverage --coverage-reporter=text --coverage-reporter=lcov",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "check": "npm run typecheck && npm run lint && npm run test",
    "stryker": "stryker run",
    "stryker:debug": "stryker run --logLevel debug",
    "stryker:self": "stryker run -c stryker-self.config.mjs",
    "docs": "typedoc --out docs/typedoc src/index.ts",
    "docs:watch": "typedoc --watch --out docs/typedoc src/index.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "stryker",
    "stryker-plugin",
    "stryker-test-runner",
    "bun",
    "mutation-testing"
  ],
  "author": "Stryker Bun Runner Contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stryker-mutator/stryker-bun.git"
  },
  "bugs": {
    "url": "https://github.com/stryker-mutator/stryker-bun/issues"
  },
  "homepage": "https://github.com/stryker-mutator/stryker-bun#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "src",
    "schema",
    "README.md"
  ],
  "peerDependencies": {
    "@stryker-mutator/core": "^9.0.0",
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@stryker-mutator/api": "^9.0.1",
    "@stryker-mutator/util": "^9.0.1",
    "execa": "^9.6.0",
    "glob": "^11.0.3",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@stryker-mutator/core": "^9.0.1",
    "@types/node": "^22.0.0",
    "@types/semver": "^7.5.0",
    "@types/source-map": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "c8": "^10.1.3",
    "eslint": "^9.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-security": "^3.0.1",
    "source-map": "^0.7.4",
    "typescript": "^5.0.0"
  }
}
